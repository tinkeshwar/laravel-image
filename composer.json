{
    "name": "tinkeshwar/imager",
    "description": "This is a laravel package to create thumbs of any desired size on the fly from an existing image.",
    "license": "MIT",
    "version": "2.2.1",
    "keywords": ["image", "gd", "laravel", "laravel-image", "image-thumb"],
    "authors": [
        {
            "name": "Tinkeshwar Singh",
            "email": "tinkeshwar@gmail.com",
            "homepage": "https://tinkeshwar.com/"
        }
    ],
    "autoload": {
        "psr-4": {
          "Tinkeshwar\\Imager\\": "src/"
        },
        "files":[
            "helper/imager.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tinkeshwar\\Imager\\ImagerServiceProvider"
            ],
            "aliases": {
                "Imager": "Tinkeshwar\\Imager\\Facades\\Imager"
            }
        }
    },
    "require": {
        "php": ">=7.0.0",
        "ext-gd": "*"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing."
    }
}
